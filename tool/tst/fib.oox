class Main
{
    static void main()
    {
        int x := Main.fib(7);
        assert x == 13;
    }

    static int fib(int n)
        requires(n >= 0)
    {
        if (n == 0) return 0;
        else if (n == 1) return 1;
        else
        {
            int a := Main.fib(n - 1);
            int b := Main.fib(n - 2);
            return a + b;
        }
    }
}